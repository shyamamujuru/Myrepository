{
	"name": "Triggerpipeline",
	"properties": {
		"activities": [
			{
				"name": "Copy Data1",
				"type": "Copy",
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"typeProperties": {
					"source": {
						"type": "AzureSqlSource"
					},
					"sink": {
						"type": "AzureSqlSink",
						"preCopyScript": "TRUNCATE TABLE [AdfDemo].[Product]"
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "AzureSqlTableProduct",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "AzureSqlDemoProduct",
						"type": "DatasetReference"
					}
				]
			}
		]
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}